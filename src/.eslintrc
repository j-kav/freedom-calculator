{
  "extends": "vue",
  "rules": {
    "indent": [
      "error",
      4
    ],
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ]
  },
  "parserOptions": {
    "ecmaVersion": 2017
  }
}